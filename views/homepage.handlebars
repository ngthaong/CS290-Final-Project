
</div>
<div id = "playersFields">
        <div class="playerinput">
            <label for="playerx" class="playerx-input">Player X Name</label>
            <div class="player-input-element">
            <input type="text" name="playerx" id="playerx" class="playerx-input">
            </div>
        </div>
        <div class="playerinput">
            <label for="playerO" class="playerO-input">Player O Name</label>
            <div class="player-input-element">
            <input type="text" name="playerO" id="playerO" class="playerO-input">
            </div>
        </div>
        <div>
            <button id="ResetButton" class="reset-button">Reset Names</button>
        </div>
    </div>    
    <section class="toggle-container">
            <button onclick="toggleTheme()" class="toggle-theme">Toggle Theme</button>
    </section>  
    <div id="Mainboxs">
        <div id="gameContainer">
            <h1 id="ticTitle">Tic Tac Toe</h1>
            <div id="cellContainer">
                <div cellIndex="0" class="cell"></div>
                <div cellIndex="1" class="cell"></div>
                <div cellIndex="2" class="cell"></div>
                <div cellIndex="3" class="cell"></div>
                <div cellIndex="4" class="cell"></div>
                <div cellIndex="5" class="cell"></div>
                <div cellIndex="6" class="cell"></div>
                <div cellIndex="7" class="cell"></div>
                <div cellIndex="8" class="cell"></div>
            </div>
            <h2 id="statusText"></h2>
            <button id="restartBtn">Restart</button>
        </div>
        <div id="scorebox">
        <h1 id="scoreSheet"> Most Wins in a single Go!: </h1>
            <ol>
                {{#each allData}}
                    {{> leaderboard}}
                {{/each}}
            </ol>
        </div>
    </div> 
</body>
</html>